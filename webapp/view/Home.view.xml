<mvc:View controllerName="sap.ui.demo.empdirectory.controller.Home"
xmlns="sap.m"
xmlns:mvc="sap.ui.core.mvc">
<Page title="Home Page">
    <content>
        <HBox justifyContent="Start" class="sapUiResponsiveContentPadding">
            <Button text="Display Not Found" class="sapUiTinyMargin" press=".onDisplayNotFoundBtnPress"/>
            <Button text="Show Employee List" class="sapUiTinyMargin" press=".onShowEmployeeBtnPress"/>
            <Button text="Show Employee Oveview" class="sapUiTinyMargin" press=".onShowEmpOverviewBtnPress"/>
        </HBox>
    </content>
</Page>
</mvc:View>