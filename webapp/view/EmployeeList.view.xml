<mvc:View
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    controllerName="hds.ui5.employeedirectory.controller.EmployeeList">
    <Page 
        title="Employee List Page"
        showNavButton="true"
        navButtonPress=".onNavBack">
        <Button text="EmployeeList page"/>
        <List items="{/employees}">
            <items>
                <StandardListItem title="{FirstName}"/>
            </items>
        </List>
    </Page>
</mvc:View>